/*Extract the first half of a string of even length*/


import java.util.*;

public class Main{
    
    public static void main(String[] args){
        
        String a,b;
        Scanner sc = new Scanner(System.in);
        a = sc.nextLine();
        b = sc.nextLine();
        
        int x = a.length();
        int y = b.length();
        
        String c = a.substring(0,1) + b.substring(y-1,y);
        System.out.println(c);
    }
    
    
    
}
