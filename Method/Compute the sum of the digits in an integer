import java.util.*;

public class Main
{
	public static void main(String[] args) {
		
		int n;
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		
		int sum = 0,rem;
		
		while(n>0){
		    rem = n % 10;
		    sum = sum + rem;
		    n = n / 10;
		}
		
		System.out.println(sum);
		
		
		
	}
}
