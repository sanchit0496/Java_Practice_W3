/*
Accepts three integers from the user and return true if two or more of them have the same rightmost digit
*/


import java.util.*;

public class Main
{

  public static void main (String[] args){

        int a,b,c,x,y,z;
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();
        
        x = a%10;
        y = b%10;
        z = c%10;
        
        System.out.println(test(x,y,z,true));
    }    
        
        public static boolean test(int x, int y , int z, boolean abc){
            return((x == y) || (x == z) || (y == z));
        }
 
 



}
