import java.util.*;

public class Main
{
	public static void main(String[] args) {
		
		String s;
		Scanner sc = new Scanner(System.in);
		s = sc.nextLine();
        int a = s.length();
        
        int count = 1;
        
        for(int i = 0; i < a; i++){
            if(s.charAt(i) == ' '){
                count++;
            }    
        }
	    
		System.out.println(count);
		
	}
}
