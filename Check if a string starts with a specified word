/*Check if a string starts with a specified word*/


import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
		String s;
        Scanner sc = new Scanner(System.in);
        s = sc.nextLine();
        String c;
        c = sc.nextLine();
        
        if(s.startsWith(c)){
            System.out.println("True");
        }else{
            System.out.println("False");
        }
		
	}
}
