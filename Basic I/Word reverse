import java.util.*;

public class Main{

    public static void main(String[] args){
        
        String word;
        Scanner sc = new Scanner(System.in);
        word = sc.nextLine();
          
        String result = "";
        
        char[] chars = word.toCharArray();
        
        for(int i = chars.length - 1; i>=0; i--){ 
            result += chars[i];
        }
        System.out.println(result);
    }
    
}
